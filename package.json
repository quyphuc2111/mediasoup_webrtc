{
  "name": "screensharing-webrtc-mediasoup",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:student": "tsc && vite build --config vite.config.student.ts && node scripts/rename-student-html.js",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:build": "cd src-tauri && tauri build",
    "tauri:build:student": "cd src-tauri && tauri build --config tauri.student.conf.json",
    "prepare:binaries": "node scripts/prepare-binaries.js",
    "build:teacher": "npm run prepare:binaries && npm run build && npm run tauri:build",
    "build:student-app": "npm run build:student && npm run tauri:build:student",
    "build:all": "npm run build:teacher && npm run build:student-app",
    "server:install": "cd mediasoup-server && npm install",
    "server:dev": "cd mediasoup-server && npm run dev",
    "server:build": "cd mediasoup-server && npm run build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "mediasoup-client": "^3.7.17"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "@tauri-apps/cli": "^2"
  }
}
